source=$1

if [ -z "$source" ]; then
    echo "usage: $0 <source> [<dest>]" >&2
    return 1
fi

dest=${2-${source:r}.list}

warehouse_path RVCT Programs 4.1 :oban41a >/dev/null

if [ "$dest" = "-" ]; then
  `warehouse_path RVCT Programs 4.1 :oban41a`/linux-pentium-dbg/fromelf --new-encoding2 -c $source | less
else
  `warehouse_path RVCT Programs 4.1 :oban41a`/linux-pentium-dbg/fromelf --new-encoding2 -c $source > $dest
fi

