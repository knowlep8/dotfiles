Installation

	cd ~
	git clone git@github.com:knowlep8/dotfiles.git

	cd dotfiles

	ln -s ~/dotfiles/vimrc ~/.vimrc
	ln -s ~/dotfiles/.vim ~/.vim
	ln -s ~/dotfiles/zshrc ~/.zshrc
	ln -s ~/dotfiles/.zsh ~/.zsh
	ln -s ~/dotfiles/.zfunc ~/.zfunc
        ln -s ~/dotfiles/.tmux.conf ~/.tmux.conf
        
